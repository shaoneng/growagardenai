{
  "name": "growagarden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node scripts/build-data.js && next build",
    "build:cf": "node scripts/build-data.js && next build",
    "preview:cf": "npx wrangler pages dev out --compatibility-date=2024-01-15",
    "deploy:cf": "npx wrangler pages deploy out",
    "start": "next start",
